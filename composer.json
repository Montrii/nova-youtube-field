{
    "name": "montri/youtube-video-field",
    "description": "A Field allowing you to add Youtube Videos to your Nova Resources!",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Montri\\VideoYoutube\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Montri\\VideoYoutube\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
